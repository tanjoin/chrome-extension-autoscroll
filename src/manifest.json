{
  "manifest_version": 2,
  "name": "autoscroll",
  "version": "0.0.1",
  "description": "最下部にスクロールするもの",
  "icons": {
    "128": "img/128.png",
    "48": "img/48.png",
    "16": "img/16.png"
  },
  "content_scripts": [
    {
      "matches": [ "http://*/*", "https://*/*" ],
      "js": [ "main.js" ]
    }
  ]
}